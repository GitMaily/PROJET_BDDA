
public class PageId {
	
	public static int FileIdx; // id du fichier
	public int PageIdx; // indice de la page 
	
	public PageId(int PageIdx) {
		
		PageId.FileIdx++;
	}
	

	public PageId() {
		
	}
	
	
	public int getFileIdx() {
		return FileIdx;
	}
	public void setFileIdx(int fileIdx) {
		FileIdx = fileIdx;
	}
	public int getPageIdx() {
		return PageIdx;
	}
	public void setPageIdx(int pageIdx) {
		PageIdx = pageIdx;
	}
	
	

}
