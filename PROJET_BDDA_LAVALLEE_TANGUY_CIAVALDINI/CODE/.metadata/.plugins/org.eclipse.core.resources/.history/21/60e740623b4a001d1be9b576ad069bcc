
public class RelationInfo {
	
	private String nom;
	private int nb_col;
	private String[] nom_col;
	private String[] type_col;
	
	
	public RelationInfo(String nom, int nb_col, String[] nom_col, String[] type_col) {
		this.nom=nom;
		this.nb_col=nb_col;
		this.nom_col=nom_col;
		this.type_col=type_col;
	}
}
